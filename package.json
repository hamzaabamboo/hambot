{
  "name": "hambot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "build:nest": "nest build --webpack",
    "build:next": "next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS='--max-old-space-size=1024' nest start -e 'node --experimental-wasm-threads'",
    "start:dev": "nest start --watch -e 'node --experimental-wasm-threads'",
    "start:debug": "nest start --debug --watch  -e 'node --experimental-wasm-threads'",
    "start:hot": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:prod": "NODE_OPTIONS='--max-old-space-size=1024' node --experimental-wasm-threads dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@discordjs/voice": "^0.7.5",
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.1",
    "@line/bot-sdk": "^7.4.0",
    "@mediafish/rtmp-server": "^0.1.1",
    "@nestjs/axios": "^0.0.4",
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/platform-fastify": "^8.2.5",
    "@nestjs/schedule": "^1.0.2",
    "canvas": "^2.8.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "fast-xml-parser": "^4.0.1",
    "fastify-nextjs": "^7.1.1",
    "fb": "^2.0.0",
    "gaxios": "^4.3.2",
    "he": "^1.2.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.27.0",
    "needle": "^3.0.0",
    "next": "^12.0.9",
    "node-ical": "^0.14.1",
    "promptpay-qr": "^0.4.4",
    "qr-image": "^3.2.0",
    "rc-slider": "9.7.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rrule": "2.6.8",
    "rtmp-server": "^0.2.0",
    "rxjs": "^7.5.2",
    "trello": "^0.11.0",
    "ytdl": "^1.3.3",
    "ytdl-core": "^4.10.0",
    "ytsr": "^3.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.5",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/qr-image": "^3.2.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.2.1",
    "ts-jest": "27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
