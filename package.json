{
  "name": "hambot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "build:nest": "nest build --webpack",
    "build:next": "next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS='--max-old-space-size=1024' nest start -e 'node --experimental-wasm-threads ",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch  -e 'node --experimental-wasm-threads'",
    "start:hot": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:prod": "NODE_OPTIONS='--max-old-space-size=1024' node --experimental-wasm-threads dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json",
    "db:create": "pnpm typeorm migration:create -n",
    "db:migrate": "pnpm typeorm migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.0.1",
    "@discordjs/voice": "^0.16.0",
    "@fastify/nextjs": "^10.0.1",
    "@fastify/static": "^6.10.2",
    "@ffmpeg/core": "^0.12.3",
    "@ffmpeg/ffmpeg": "^0.12.6",
    "@line/bot-sdk": "^7.5.2",
    "@mediafish/rtmp-server": "^0.1.4",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.3",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.2.3",
    "@nestjs/platform-express": "^10.2.3",
    "@nestjs/platform-fastify": "^10.2.3",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/typeorm": "^10.0.0",
    "canvas": "^2.11.2",
    "discord-api-types": "^0.37.55",
    "discord.js": "^14.13.0",
    "eslint-config-prettier": "^9.0.0",
    "fast-xml-parser": "^4.2.7",
    "fastify": "^4.22.0",
    "fb": "^2.0.0",
    "gaxios": "^6.1.0",
    "he": "^1.2.0",
    "libsodium-wrappers": "^0.7.11",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "moment": "^2.29.4",
    "needle": "^3.2.0",
    "nestjs-spelunker": "^1.1.5",
    "next": "^13.4.19",
    "node-ical": "^0.16.1",
    "promptpay-qr": "^0.5.0",
    "qr-image": "^3.2.0",
    "rc-slider": "10.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rrule": "2.7.2",
    "rtmp-server": "^0.2.0",
    "run-script-webpack-plugin": "^0.2.0",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.6",
    "trello": "^0.11.0",
    "typeorm": "^0.3.17",
    "ytdl": "^1.4.1",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.7",
    "@types/qr-image": "^3.2.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "roots": [
      "src"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
