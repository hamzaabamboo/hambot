  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "build:nest": "nest build --webpack",
    "build:next": "next build --no-lint",
    "format": "prettier \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "pnpm format --check",
    "format:fix": "pnpm format --write",
    "start": "NODE_OPTIONS='--max-old-space-size=1024' nest start -e 'node --experimental-wasm-threads ",
    "dev": "pnpm start:dev",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch  -e 'node --experimental-wasm-threads'",
    "start:hot": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:prod": "NODE_OPTIONS='--max-old-space-size=1024' node --experimental-wasm-threads dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json",
    "db:create": "pnpm typeorm migration:create -n",
    "db:migrate": "pnpm typeorm migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "pnpm lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
